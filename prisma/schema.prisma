generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model player {
  name     String @db.VarChar(100) @unique
  password String @db.VarChar(100)
  hp       Float
  pos      String @default("{x: 0, y: 0}") @db.VarChar(100)
  id       Int    @id @default(autoincrement())

  @@map("players")
}
